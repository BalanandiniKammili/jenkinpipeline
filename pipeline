pipeline {
    agent any

    stages {
        stage('validate') {
            steps {
                echo 'I am validating the code'
            }
        }
     stage('Compile') {
            steps {
                echo 'I am compiling the code'
            }
        }
     stage('test') {
            steps {
                echo 'I am instaling the code'
            }
        }
      stage('Deploy') {
            steps {
                echo 'I am deploying the code'
            }
        }
    }
}
